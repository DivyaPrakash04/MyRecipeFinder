{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "redirects": [
    {
      "from": "/api/*",
      "to": "https://myrecipefinder-backend-production.up.railway.app/api/:splat",
      "status": 200
    }
  ]
}
